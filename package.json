{
  "name": "db-studio",
  "version": "1.0.0",
  "description": "DB Studio - Data visualization and dashboard builder",
  "main": "dist/main/main.js",
  "scripts": {
    "start": "npm install && npm run build && npm run electron",
    "dev": "concurrently \"npm run build:watch\" \"npm run electron:dev\"",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "webpack --mode production",
    "build:watch": "webpack --mode development --watch",
    "electron": "electron .",
    "electron:dev": "cross-env NODE_ENV=development electron .",
    "package": "electron-builder",
    "clean": "rimraf dist"
  },
  "keywords": [
    "data",
    "dashboard",
    "visualization",
    "electron"
  ],
  "author": "DB Studio",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "electron": "^25.0.0",
    "electron-builder": "^24.0.0",
    "html-webpack-plugin": "^5.5.0",
    "rimraf": "^5.0.0",
    "ts-loader": "^9.4.0",
    "typescript": "^5.0.0",
    "webpack": "^5.80.0",
    "webpack-cli": "^5.1.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.15.0",
    "@types/d3": "^7.4.3",
    "@types/html2canvas": "^0.5.35",
    "@types/papaparse": "^5.3.14",
    "css-loader": "^6.8.0",
    "d3": "^7.9.0",
    "date-fns": "^2.30.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "style-loader": "^3.3.0",
    "xlsx": "^0.18.5"
  },
  "build": {
    "appId": "com.dbstudio.app",
    "productName": "DB Studio",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.productivity"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
